<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false" android:state_focused="false">
        <shape>
            <gradient
                android:centerY="0.2"
                android:startColor="#FF5722"
                android:centerColor="#65E6C8C8"
                android:endColor="#00130D0D"
                android:angle="270"
                />
            <stroke
                android:width="1.7dp"
                android:color="#EE4B17" />
            <corners
                android:radius="35dp" />
        </shape>
    </item>
</selector>